{
    "name": "agent-api/rest-api",
    "description": "Agent Management REST API with Laravel components",
    "type": "project",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^11.0",
        "illuminate/events": "^11.0",
        "laravel/sanctum": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Models\\": "models/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
